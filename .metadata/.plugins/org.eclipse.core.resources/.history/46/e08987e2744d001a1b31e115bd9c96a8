class HashMap1
{
	public static void main(String args[])
	{
		HashMap<Integer, Integer>h1=new HashMap<>();
		Scanner sc=new Scanner(System.in);
		System.out.println("enter no of elements to be entered");
		int n=sc.nextInt();
		for(int i=0;i<n;i++)
